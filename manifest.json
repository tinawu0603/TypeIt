{
  "manifest_version": 2,

  "name": "TypeIt!",
  "description": "This extension allows the user to customize the current page.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icons/icon1.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
